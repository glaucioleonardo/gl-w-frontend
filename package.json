{
  "name": "gl-w-frontend",
  "version": "5.1.0",
  "description": "Common code for using among web front-end development such as ES6+ and TypeScript",
  "main": "lib/bundle.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "shx rm -rf dist lib",
    "production": "npm run clean && npm run build",
    "fast-build": "tsc",
    "test": "npm run production && npm pack",
    "lint": "eslint . --ext .ts",
    "build-index": "tsc -p ./tsconfig-index.json",
    "build-es6": "tsc -p ./tsconfig-es6.json",
    "build": "npm run build-es6 && npm run build-index"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glaucioleonardo/gl-w-frontend.git"
  },
  "keywords": [
    "javascript tool",
    "typescript tool",
    "front-end",
    "pure javascript",
    "array tool",
    "browser tool",
    "autocomplete tool",
    "fullscreen",
    "attachment tool",
    "dom tool",
    "excel",
    "array",
    "attachment",
    "browser",
    "combobox",
    "autocomplete",
    "date",
    "dom elements",
    "inputs",
    "menu",
    "string",
    "url"
  ],
  "bugs": "https://github.com/glaucioleonardo/gl-w-frontend/issues",
  "author": "Gl√°ucio Leonardo Sant'ana",
  "license": "ISC",
  "homepage": "https://www.glauciosantana.com",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "^7.28.0",
    "shx": "^0.3.4",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
