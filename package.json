{
  "name": "gl-w-frontend",
  "version": "1.3.2",
  "description": "Common code for using among web front-end development such as pure JavaScript, ES6+ and TypeScript",
  "main": "lib/",
  "types": "lib/",
  "scripts": {
    "clean": "shx rm -rf dist lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-clean": "npm run clean && tsc && tsc -m es6 --outDir lib/esm",
    "build": "tsc && tsc -m es6 --outDir lib/esm",
    "fast-build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glaucioleonardo/gl-w-frontend.git"
  },
  "keywords": [
    "javascript tool",
    "typescript tool",
    "front-end",
    "pure javascript",
    "array tool",
    "browser tool",
    "autocomplete tool",
    "fullscreen",
    "attachment tool",
    "dom tool"
  ],
  "author": "Gl√°ucio Leonardo Sant'ana",
  "license": "ISC",
  "homepage": "https://glauciosantana.com",
  "dependencies": {
    "@types/node": "^14.0.5",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "screenfull": "^5.0.2"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.38",
    "@types/jqueryui": "^1.12.13",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "codelyzer": "^5.2.2",
    "eslint": "^7.1.0",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "shx": "^0.3.2",
    "ts-node": "~8.3.0",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
